<% case @station.state %>
<% when 1 %>
  <% case @station.channel %>
  <% when 0 %>
    $(".current_ch").text("<%= "0#{@station.channel}" %>");
    $(".ch_info").removeClass("reverse_color");
    $(".dup_info").hide();
  <% when 1..3 %>
    $(".current_ch").text("<%= "0#{@station.channel}" %>");
    $(".ch_info").removeClass("reverse_color");
    $(".dup_info").show();
  <% when 3..9 %>
    $(".current_ch").text("<%= "0#{@station.channel}" %>");
    $(".ch_info").removeClass("reverse_color");
    $(".dup_info").hide();
  <% when 10..15 %>
