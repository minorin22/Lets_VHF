<% when 2 %>
  $(".current_ch").text("<%= @station.tmp_ch %>-");
<% when 5 %>
  number = <%= @num_btn %>;
  if (number == 0) {
    $.post(`/stations/${gon.station_id}/other_ships_list`);
  }
  if (number == 8) {
    $.post(`/stations/${gon.station_id}/self_diagnosis`);
  }
<% else %>
<% end %>
