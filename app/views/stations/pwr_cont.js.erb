<% case @station.state %>
<% when 1 %>

  $(".start_up_screen").show();

  setTimeout(function(){
    $(".start_up_screen").hide();
    $(".home_screen").show();
    $(".current_ch").text("<%= @station.channel %>");
    $(".dup_info").hide();
    $(".pwr_info").hide();
    if($(".ch_info").not("reverse_color")){
      $(".ch_info").addClass("reverse_color");
    };
  },2200);

<% else %>
<% end %>
